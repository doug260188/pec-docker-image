pipeline {
    agent any

    stages {
        stage('Run Container') {
            steps {
                script {
                    //Baixando dockerfile
                    sh 'wget -O https://raw.githubusercontent.com/doug260188/pec-docker-image/main/dockeflie'

                    //Construindo imagem
                    sh 'docker build -t doug260188/debina:latest .'
                }
                
            }
        }
    }
}

